      <Button layoutX="313.0" layoutY="270.0" mnemonicParsing="false" onAction="#deleteFacture" prefHeight="25.0" prefWidth="83.0" text="Delete">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button layoutX="419.0" layoutY="269.0" mnemonicParsing="false" onAction="#getCommandeById" prefHeight="25.0" prefWidth="83.0" text="Search">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="119.0" layoutY="317.0" mnemonicParsing="false" onAction="#reset" prefHeight="25.0" prefWidth="83.0" text="Reset">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button onAction="#printFacture" layoutX="208.0" layoutY="317.0" mnemonicParsing="false" text="Print Facture">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="316.0" layoutY="317.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="83.0" text="Button">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="418.0" layoutY="317.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="86.0" text="Button">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
