      <Label layoutX="119.0" layoutY="103.0" prefHeight="17.0" prefWidth="93.0" text="Designation">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font></Label>
      <Label layoutX="118.0" layoutY="144.0" onKeyPressed="#fillSousTotalPriceTextField" prefHeight="17.0" prefWidth="77.0" text="Quantite">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font></Label>
      <Label layoutX="117.0" layoutY="183.0" prefHeight="17.0" prefWidth="86.0" text="Sous Total">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font></Label>
      <TextField fx:id="numeroCommande" layoutX="226.0" layoutY="26.0" prefHeight="25.0" prefWidth="207.0" />
      <TextField fx:id="quantite" layoutX="227.0" layoutY="140.0" onKeyReleased="#fillSousTotalPriceTextField" prefHeight="25.0" prefWidth="207.0" />
      <TextField fx:id="sousTotal" layoutX="227.0" layoutY="179.0" prefHeight="25.0" prefWidth="207.0" />
      <ComboBox fx:id="numeroFacture" layoutX="227.0" layoutY="62.0" prefHeight="25.0" prefWidth="207.0" />
