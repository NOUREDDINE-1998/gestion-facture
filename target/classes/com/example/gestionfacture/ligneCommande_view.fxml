      <Button layoutX="119.0" layoutY="269.0" mnemonicParsing="false" onAction="#addLigneCommande" prefHeight="25.0" prefWidth="83.0" text="Add">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button  onAction="#updateLigneCommande" layoutX="208.0" layoutY="269.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="98.0" text="Update">
